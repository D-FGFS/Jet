<?xml version="1.0"?>

<PropertyList>

 <path>tach.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
  <emission>
   <red>0.8</red>
   <green>0.8</green>
   <blue>0.8</blue>
    <condition>
<and>
      <property>/controls/electric/battery-switch</property>
<property>/controls/lighting/instrument-lights</property>
</and>
    </condition>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/engines/engine[1]/n2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>-128.6 </dep></entry>     
   <entry><ind>  0.1</ind><dep>-123.85</dep></entry>  
   <entry><ind> 60</ind><dep> -70 </dep></entry>        
   <entry><ind> 100</ind><dep> 70 </dep></entry>     
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


