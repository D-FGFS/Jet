<?xml version="1.0"?>

<PropertyList include="ea-next-base.xml">
    <sim>
        <description>Jet</description>
        
         <flight-model>yasim</flight-model>
        <aero>jet</aero>
        <fuel-fraction>0.5</fuel-fraction>
        <startup>
            <splash-texture>Aircraft/Jet/splash.png</splash-texture>
        </startup>
   <weight n="0">
        <name>Crew</name>
        <weight-lb>360</weight-lb>
        <max-lb>500</max-lb>
        <min-lb>100</min-lb>
    </weight>

    <weight n="1">
        <name>Passengers, rows 1-7</name>
        <weight-lb>2464</weight-lb> <!-- average 176lb (80kg) per person, mid density: 2/3 seats used: 7 rows @ 2 passengers-->
        <max-lb>5565</max-lb> <!--maximal 265lb (120kg) per person, 7 rows @ 3 passengers-->
        <min-lb>0</min-lb>
    </weight>

    <weight n="2">
        <name>Passengers, rows 8-13</name>
        <weight-lb>2112</weight-lb> <!-- average 176lb (80kg) per person, mid density: 2/3 seats used: 6 rows @ 2 passengers-->
        <max-lb>4770</max-lb> <!--maximal 265lb (120kg) per person, 6 rows @ 3 passengers-->
        <min-lb>0</min-lb>
    </weight>

    <weight n="3">
        <name>Passengers, rows 14-19</name>
        <weight-lb>2112</weight-lb> <!-- average 176lb (80kg) per person, mid density: 2/3 seats used: 6 rows @ 2 passengers-->
        <max-lb>4770</max-lb> <!--maximal 265lb (120kg) per person, 6 rows @ 3 passengers-->
        <min-lb>0</min-lb>
    </weight>

    <weight n="3">
        <name>Baggage</name>
        <weight-lb>1254</weight-lb> <!--average around 33lbs (15kg) baggage per person, 38 (2/3) passengers-->
        <max-lb>3762</max-lb> <!--maximal around 66lbs (30kg) baggage per person, 57 (full) passengers-->
        <min-lb>0</min-lb>
    </weight>
    <!--<engines>
        <engine n="0">
            <thruster>
                <torque type="float">0.0</torque>
            </thruster>
        </engine>
        <engine n="1">
            <thruster>
                <torque type="float">0.0</torque>
            </thruster>
        </engine>
    </engines>-->
<instrumentation>
    <mk-viii>                          <!-- http://wiki.flightgear.org/index.php/GPWS -->
      <serviceable>true</serviceable>  <!-- EGPWS_ENABLE -->
      <configuration-module>
       <category-1>   1 </category-1>  <!-- AIRCRAFT_MODE_TYPE_SELECT -->
       <category-8>   3 </category-8>  <!-- RADIO_ALTITUDE_INPUT_SELECT -->
      </configuration-module>
      <inputs>                         <!-- Module I/O -->
       <arinc429>
        <decision-height-ncd> false </decision-height-ncd>
        <decision-height>150</decision-height>
       </arinc429>
      </inputs>
          <speaker>
                <volume>1</volume>
                <max-dist>2</max-dist>
                <reference-dist>0.5</reference-dist>
                <position>
                    <x>-4.5</x>
                    <y>0</y>
                    <z>0.5</z>
                </position>
          </speaker>
    </mk-viii>
    <eng-gauge>
        <itt n="0" type="double">0</itt>
        <itt n="1" type="double">0</itt>
        <torque n="0" alias="/engines/engine/torque-ftlb"/>
        <torque n="1" alias="/engines/engine[1]/torque-ftlb"/>
        <prop n="0" alias="/engines/engine/rpm"/>
        <prop n="1" alias="/engines/engine[1]/rpm"/>
        <turbine n="0" alias="/engines/engine/n1"/>
        <turbine n="1" alias="/engines/engine[1]/n1"/>
        <fuel-pph n="0" type="double">0</fuel-pph>
        <fuel-pph n="1" type="double">0</fuel-pph>
        <oil-psi-norm n="0" type="double">0</oil-psi-norm>
        <oil-psi-norm n="1" type="double">0</oil-psi-norm>
        <oil-temp-norm n="0" type="double">0</oil-temp-norm>
        <oil-temp-norm n="1" type="double">0</oil-temp-norm>
    </eng-gauge>
    <fuel-gauge>
        <LH-qty type="double">0</LH-qty>
        <RH-qty type="double">0</RH-qty>
    </fuel-gauge>
    <efis>
        <inputs>
            <arpt>1</arpt>
        </inputs>
    </efis>
    </instrumentation> 

</sim>


   
    <nasal>
    <doors>
      <file>Nasal/doors.nas</file>
    </doors>
        <systems>
            <file>Aircraft/Jet/Models/Panel/Instruments/ND/ND.nas</file>
            <file>Nasal/Electrical.nas</file>
            <file>Nasal/systems.nas</file>
            <file>Nasal/light.nas</file>
        </systems>
    </nasal>

</PropertyList>
